{
  "name": "@xarples/users-db",
  "version": "1.0.0",
  "description": "Database module of xarples user service",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src --ext .ts,.js",
    "lint:fix": "eslint ./src --ext .ts,.js --fix",
    "migration:run": "sequelize-cli db:migrate",
    "migration:generate": "sequelize-cli migration:generate",
    "introspect": "prisma2 introspect",
    "postinstall": "npm run build",
    "test": "NODE_ENV=test ava --config ava.config.js --color",
    "test:coverage": "nyc --reporter=lcov --reporter=text-summary npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "sequelize-cli": "^5.5.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@prisma/photon": "2.0.0-preview019",
    "@xarples/config": "^1.0.0",
    "pg": "^7.17.0",
    "pg-hstore": "^2.3.3",
    "prisma2": "^2.0.0-preview019",
    "sequelize": "^5.21.3",
    "sqlite3": "^4.1.1"
  }
}
